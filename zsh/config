# colors
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagacad

export PS1='%m:%3~$(vcs_info_for_prompt)%(?..[%?])%# '
export PYTHONPATH=.:${PYTHONPATH}

fpath=(~/.zsh/functions $fpath)
autoload -U ~/.zsh/functions/*(:t)

setopt promptsubst
setopt histignorealldups

HISTSIZE=1000
SAVEHIST=1000
HISTFILE=~/.history
