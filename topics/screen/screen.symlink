startup_message off

hardstatus alwayslastline "%{=b}%{G} Screen(s): %-w%{dw}%n %t%{dd}%{G}%+w"
startup_message off

msgwait 1

## Bind for windows past 9

bind  ! select 11
bind  @ select 12
bind \# select 13
bind  $ select 14
bind  % select 15
bind \^ select 16
bind  & select 17
bind  * select 18
bind  ( select 19
bind  ) select 10

